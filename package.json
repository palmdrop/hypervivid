{
  "name": "hypervivid",
  "version": "0.0.1",
  "scripts": {
    "start": "bash scripts/start.sh",
    "start:noprocess": "bash scripts/start.sh -n",
    "dev": "vite dev --host",
    "process": "bash ./scripts/process.sh",
    "gen": "bash ./scripts/generate.sh",
    "prebuild": "npm install",
    "build": "bash ./scripts/build.sh",
    "postbuild": "node ./scripts/postbuild.cjs",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@crownframework/svelte-error-boundary": "^1.0.3",
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/adapter-static": "^1.0.0",
    "@sveltejs/kit": "^1.0.0",
    "@types/dat.gui": "^0.7.7",
    "@types/lodash": "^4.14.183",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "^17.0.27",
    "@types/three": "^0.143.2",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "chokidar-cli": "^3.0.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "lodash.pick": "^4.4.0",
    "mdsvex": "^0.10.6",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "rxjs": "^7.5.6",
    "svelte": "^3.44.0",
    "svelte-check": "^3.0.1",
    "svelte-preprocess": "^5.0.0",
    "svelte-select": "^5.0.1",
    "three": "^0.139.2",
    "ts-dedent": "^2.2.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2",
    "vite": "^4.0.0"
  },
  "type": "module",
  "dependencies": {
    "@tweenjs/tween.js": "^18.6.4",
    "dat.gui": "^0.7.9",
    "fast-simplex-noise": "^4.0.0",
    "lodash": "^4.17.21",
    "lodash.throttle": "^4.1.1",
    "postprocessing": "^6.28.1",
    "three-kawase-blur": "github:ycw/three-kawase-blur"
  },
  "peerDependencies": {
    "vite": "^4.0.0"
  }
}
